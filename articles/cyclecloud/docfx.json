{
  "build": {
    "content":
       [
         {
             "files": ["breadcrumb/toc.yml"]
         },
         {
           "files": ["**/**.md",
                     "**/**.yml"],
           "exclude": ["**/obj/**"]
         }
       ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
    "brand": "azure",
    "searchScope": ["CycleCloud"],
      "breadcrumb_path": "/azure/cyclecloud/breadcrumb/toc.json",
      "extendBreadcrumb": true
    },
    "fileMetadata": {},
    "template": [],
    "dest": "cycle_docs_v1"
  }
}
